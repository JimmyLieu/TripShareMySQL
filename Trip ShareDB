CREATE TABLE DESTINATION (
	DESTINATION_ID VARCHAR(10) NOT NULL,
	PHONE_NUMBER VARCHAR(17),
    COUNTRY VARCHAR (100),
    STATE VARCHAR (100),
    CITY VARCHAR(100),
    NAME VARCHAR(100),
    ADDRESS VARCHAR(100),
    OPENING_HOURS TIME, 
    CLOSING_HOURS TIME,
    DESCRIPTION VARCHAR(100),
    RATING DECIMAL(3,2),
    COMMT_USER VARCHAR(100),
    
    PRIMARY KEY (DESTINATION_ID)
    -- COMMT_USER REFERENCES A ATTRIBUTE IN ANOTHER TABLE, SO WILL WAIT TILL TABLE IS CREATED
);
-- AUTHORIZED_MEMBER
CREATE TABLE AUTHORIZED_MEMBER (
USERNAME VARCHAR(50) NOT NULL,
EMAIL VARCHAR(100) NOT NULL,
WISH_DESTINATION VARCHAR(50),
PASSWORD VARCHAR(50) NOT NULL,
ADDRESS VARCHAR(50) NOT NULL,
COUNTRY VARCHAR(50),
CITY VARCHAR(50),
VISITED_DESTINATION VARCHAR(50),
PEOPLE_FOLLOWED INT,
 NUMBER_OF_FOLLOWERS INT,
 RANKING INT(4),
 MEMBER_STATUS VARCHAR(50),
PRIMARY KEY (USERNAME),

CONSTRAINT AUTHORIZED_MEMBER_FK FOREIGN KEY (VISITED_DESTINATION)
REFERENCES DESTINATION(DESTINATION_ID)
	ON DELETE CASCADE
	ON UPDATE CASCADE
) ;




-- PREFERRED_MEMBER
CREATE TABLE PREFERRED_MEMBER (
USERNAME VARCHAR(50) NOT NULL,
EMAIL VARCHAR(100) NOT NULL,
PRIMARY KEY (USERNAME)
);


-- REGULAR_MEMBER
CREATE TABLE REGULAR_MEMBER(
USERNAME VARCHAR(50) NOT NULL,
EMAIL VARCHAR(100) NOT NULL,
NUMBER_OF_FOLLOWERS INT,
PRIMARY KEY (USERNAME)
);



CREATE TABLE TRIP(
PLAN_ID VARCHAR(12) NOT NULL,
START_DATETIME DATETIME NOT NULL,
END_DATETIME DATETIME NOT NULL,
NAME CHAR(50) NOT NULL,
POTENTIAL_COST CHAR(9),
ARRIVAL_TIME DATETIME,
DEPARTURE_TIME DATETIME,
CREATOR_MEM CHAR(50) NOT NULL,
PRIMARY KEY (PLAN_ID),
DURATION INT,
CONSTRAINT TRIP_FK1 FOREIGN KEY (CREATOR_MEM)
REFERENCES AUTHORIZED_MEMBER(USERNAME)
	ON DELETE CASCADE
	ON UPDATE CASCADE
);



CREATE TABLE COMPRISE_OF(
PLAN_ID VARCHAR(100) NOT NULL,
DESTINATION_ID VARCHAR(100) NOT NULL,
PRIMARY KEY (PLAN_ID, DESTINATION_ID),
CONSTRAINT COMPRISE_OF_FK FOREIGN KEY (PLAN_ID)
REFERENCES TRIP(PLAN_ID)
ON DELETE CASCADE
ON UPDATE CASCADE,
CONSTRAINT COMPRISE_OF_FK2 FOREIGN KEY (DESTINATION_ID) REFERENCES DESTINATION(DESTINATION_ID)
ON DELETE CASCADE
ON UPDATE CASCADE
);

-- TABLE TO TRIP ARRIVAL.DEPARTURE TIMES
CREATE TABLE TRIP_DEPART_TIMES(
PLAN_ID CHAR(12) NOT NULL,
ARRIVAL_TIME DATETIME NOT NULL,
DEPARTURE_TIME DATETIME NOT NULL,
PRIMARY KEY (PLAN_ID),
CONSTRAINT TRIP_DEPART_TIMES_FK FOREIGN KEY (PLAN_ID)
REFERENCES TRIP(PLAN_ID)
	ON DELETE CASCADE
	ON UPDATE CASCADE
);


-- TABLE_RATES
CREATE TABLE TRIP_RATES (
    USERNAME VARCHAR(50) NOT NULL,
    PLAN_ID CHAR(12) NOT NULL,
    RATING DOUBLE,
    PRIMARY KEY (USERNAME, PLAN_ID),
    CONSTRAINT  TRIP_RATES_FK1 FOREIGN KEY (USERNAME) REFERENCES AUTHORIZED_MEMBER(USERNAME)
    ON DELETE CASCADE
	ON UPDATE CASCADE,
	CONSTRAINT TRIP_RATES_FK2 FOREIGN KEY (PLAN_ID) REFERENCES TRIP(PLAN_ID)
	ON DELETE CASCADE
	ON UPDATE CASCADE
);




-- ANTONIO’S TABLE’S CODE


CREATE TABLE TRAVEL_ATTRACTION(
	DEST_ID_TA VARCHAR(100) NOT NULL,
    
    PRIMARY KEY (DEST_ID_TA),
    
    CONSTRAINT TRAVEL_ATTRACTION_FK1 FOREIGN KEY(DEST_ID_TA)
		REFERENCES DESTINATION (DESTINATION_ID)
	ON DELETE CASCADE
    ON UPDATE CASCADE
);

CREATE TABLE RESTAURANTS(
	RESTAURANT_ATTRACTION VARCHAR(100) NOT NULL,
    RESTAURANT_WEBLINK VARCHAR(100),
    RESTAURANT_TYPE VARCHAR(100),
    PRICE_RANGE INT(100),
    RESTAURANT_NAME VARCHAR(100) NOT NULL,
    UNIQUE(RESTAURANT_NAME),
    OWNER_NUM VARCHAR(17),
    
    PRIMARY KEY (RESTAURANT_ATTRACTION, RESTAURANT_NAME),
    
    CONSTRAINT RESTAURANTS_FK1 FOREIGN KEY (RESTAURANT_ATTRACTION)
		REFERENCES TRAVEL_ATTRACTION(DEST_ID_TA)
	ON DELETE CASCADE
    ON UPDATE CASCADE
);



CREATE TABLE SHOPPING_MALL(
	MALL_ATTRACTION VARCHAR(100) NOT NULL,
    SHOPPING_MALL_NAME VARCHAR(100) NOT NULL,
    UNIQUE(SHOPPING_MALL_NAME),
    OWNER_NUMBER VARCHAR(15),
    UNIQUE(SHOPPING_MALL_NAME),
    PRIMARY KEY (MALL_ATTRACTION, SHOPPING_MALL_NAME),
    /*CONSTRAINT SHOPPING_MALL_FK1 FOREIGN KEY (SHOPPING_MALL_NAME)
		REFERENCES BUSINESS_OWNER (BUSINESS_NAME)
	ON DELETE CASCADE
    ON UPDATE CASCADE,*/
	CONSTRAINT SHOPPING_MALL_FK2 FOREIGN KEY (MALL_ATTRACTION)
		REFERENCES TRAVEL_ATTRACTION (DEST_ID_TA)
	ON DELETE CASCADE
    ON UPDATE CASCADE
);

CREATE TABLE BUSINESS_OWNER(
	BUSINESS_NAME VARCHAR(100) NOT NULL,
    RESTAURANT_NAME VARCHAR(100),
    SHOP_NAME VARCHAR(100),
    OWNER_PHONE_NUMBER VARCHAR(100) NOT NULL,
    BUSINESS_TYPE VARCHAR(100),
    OWNER_NAME VARCHAR(20),
    PRIMARY KEY(BUSINESS_NAME, OWNER_PHONE_NUMBER),
    
    CONSTRAINT BUSINESS_OWNER_FK1 FOREIGN KEY (RESTAURANT_NAME)
    REFERENCES RESTAURANTS(RESTAURANT_NAME)
    ON DELETE CASCADE
    ON UPDATE CASCADE,
    CONSTRAINT BUSINESS_OWNER_FK2 FOREIGN KEY (SHOP_NAME)
    REFERENCES SHOPPING_MALL(SHOPPING_MALL_NAME)
    ON DELETE CASCADE
    ON UPDATE CASCADE
    -- CONSTRAINT BUSINESS_OWNER_FK2 FOREIGN KEY (BUSINESS_NAME) REFERENCES SHOPPING_MALL(SHOPPING_MALL_NAME)
    );
    

	


CREATE TABLE SIGHTS(
	SIGHT_ATTRACTION VARCHAR(100) NOT NULL,
    SIGHT_WEBLINK VARCHAR(100) NOT NULL,
    TICKET_PRICE VARCHAR(100),
    
    PRIMARY KEY (SIGHT_ATTRACTION),
    
    CONSTRAINT SIGHTS_FK1 FOREIGN KEY (SIGHT_ATTRACTION)
		REFERENCES TRAVEL_ATTRACTION(DEST_ID_TA)
	ON DELETE CASCADE
    ON UPDATE CASCADE
);

DROP TABLE IF EXISTS IMAGE, COM_RATE, COMMENT;
CREATE TABLE IMAGE(
USERNAME VARCHAR(50) NOT NULL,
IMAGE_ID INT(20) NOT NULL,
IMAGE_LINK CHAR(100) NOT NULL,
CONSTRAINT IMAGE FOREIGN KEY (USERNAME)
REFERENCES AUTHORIZED_MEMBER(USERNAME)
ON DELETE CASCADE
ON UPDATE CASCADE
);

CREATE TABLE COM_RATE(
RATE_USER VARCHAR(50) NOT NULL,
USER_CMT VARCHAR(50) NOT NULL,
COM_RATING DOUBLE,
CONSTRAINT COM_RATE FOREIGN KEY (RATE_USER)
REFERENCES AUTHORIZED_MEMBER(USERNAME)
ON DELETE CASCADE
ON UPDATE CASCADE
);

CREATE TABLE COMMENT(
USERNAME VARCHAR(50) NOT NULL,
REPLIES_USER VARCHAR(50) NOT NULL,
DATE DATE,
CONTENT CHAR(100) NOT NULL,
POSTING_TIME TIME
);

CREATE TABLE EDIT_DESCRIPTION(
DES_NUM VARCHAR(10) NOT NULL,
PREFERRED_MEM_USER VARCHAR(50)
);


CREATE TABLE TRIP_MEMBERS(
PLAN_ID CHAR(12) NOT NULL,
ASSOCIATED_MEMBERS CHAR(50) NOT NULL,
PRIMARY KEY (PLAN_ID, ASSOCIATED_MEMBERS),
CONSTRAINT TRIP_MEMBERS FOREIGN KEY (PLAN_ID)
REFERENCES TRIP(PLAN_ID)
ON DELETE CASCADE
ON UPDATE CASCADE
);

CREATE TABLE PURPOSE(
PLAN_ID VARCHAR(12),
TRIP_PURPOSE VARCHAR(100),
CONSTRAINT PURPOSE_FK FOREIGN KEY (PLAN_ID)
REFERENCES TRIP(PLAN_ID)
ON DELETE CASCADE
ON UPDATE CASCADE
);

CREATE TABLE RANKINGS(
RANKING INT NOT NULL,
MINIMAGES INT,
MAXIMAGES INT
);

CREATE TABLE WAYS_OF_TRAVEL (
DESTINATION_ID INT,
TRAVEL_METHOD VARCHAR(50),
PRIMARY KEY (DESTINATION_ID)
);


